{
  "name": "gymflow-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/backend",
    "apps/frontend"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:backend dev:frontend",
    "dev:backend": "npm --workspace @gymflow/backend run start:dev",
    "dev:frontend": "npm --workspace @gymflow/frontend run dev",
    "prisma:push": "npm --workspace @gymflow/backend run prisma:push",
    "prisma:seed": "npm --workspace @gymflow/backend run prisma:seed"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}









